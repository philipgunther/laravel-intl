{
    "name": "rivalex/laravel-intl",
    "description": "Easy to use internationalization functions for Laravel",
    "keywords": [
        "laravel",
        "internationalization",
        "intl",
        "i18n",
        "l10n"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rivalex",
            "email": "info@rivalex.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "nesbot/carbon": "^2.57.0",
        "commerceguys/intl": "^1.1.1",
        "umpirsky/country-list": "^2.0.6",
        "umpirsky/locale-list": "^1.0",
        "punic/punic": "^3.7",
        "illuminate/support": "^11.8.0",
        "illuminate/events": "^11.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^v9.1.2",
        "phpunit/phpunit": "^10.5 || ^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "Rivalex\\LaravelIntl\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Rivalex\\LaravelIntl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rivalex\\LaravelIntl\\IntlServiceProvider"
            ]
        }
    }
}
